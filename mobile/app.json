{
  "expo": {
    "name": "healthmate",
    "slug": "healthmate",
    "version": "1.0.0",
    
      "scheme": "healthmate",
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/own/splashScreen.png"
      },
      "plugins": [
      "expo-router"
      
      
    ],
      "experiments": {
        "typedRoutes": true
        
          ,"tsconfigPaths": true
        
      },
    
    "orientation": "portrait",
    "icon": "./assets/own/healthcare.png",
    
     "userInterfaceStyle": "light",
    
    "splash": {
      "image": "./assets/own/hmsshd.png",
      "resizeMode": "contain",
      "backgroundColor": "#1649ff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/own/healthcare.png",
        "backgroundColor": "#1649ff"
      }
    }
  }
}